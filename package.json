{
  "name": "proto-to-frontend-interface-tool",
  "version": "1.0.0",
  "description": "PB Converter",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "mock": "node ./config/mockServer.js",
    "build": "yarn rmdist && webpack --config ./config/webpack.build.dev.js",
    "build:p": "yarn rmdist && webpack --config ./config/webpack.build.prod.js",
    "start": "webpack serve --config ./config/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production webpack serve --config ./config/webpack.dev.js",
    "analyze": "webpack --profile --json > stats.json --config ./config/webpack.prod.js && webpack-bundle-analyzer stats.json",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "prepare": "husky install",
    "web3-token-abi": "node ./config/web3TokenAbi.js",
    "rmdist": "rm -rf ./dist",
    "openapi": "node ./config/genSwagger.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangym0203/proto-to-frontend-interface-tool.git"
  },
